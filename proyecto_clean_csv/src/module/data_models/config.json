{
    "validations":{
        "validate_duplicates": true,
        "validate_nulls": true,
        "validate_types": true
    },
    "duplicates": {
        "apply": true,
        "keep": "last",
        "columns": ["Transaction ID"]
    },
    "types": {
        "apply": true
    },
    "imputation": {
        "apply_amounts": true,
        "apply_category": true
    },
    "nulls": {
        "apply": true,
        "fill_value": "NO_PROPORCIONADO",
        "columns": ["Category", "Payment Method"]
    }

}